    <modules>
      <module name="core-app" active="true"/>
      <module name="factbook" active="true"/>
      <module name="jms" active="true"/>
      <module name="adminui" active="true"/>
      <module name="memory" active="true"/>
      <module name="profiler" active="true"/>
      <module name="scheduler" active="true"/>
      <module name="advancedtextextraction" active="true"/>
      <module name="autocomplete" active="true"/>
      <module name="rest" active="true"/>
      <module name="sail" active="true"/>
      <module name="opensearch" active="true"/>
      <module name="webservice" active="true"/>
      <module name="principals" active="true"/>
      <module name="apisecurity" active="true"/>
      <module name="businesscenter" active="true"/>
      <module name="workflowvisualizer" active="true"/>
      <module name="clustering" active="true"/>
      <module name="entityextraction" active="true"/>
      <module name="languagemodel" active="true"/>
      <module name="keyphrases" active="true"/>
    </modules>
  </configuration>
</attivio>
